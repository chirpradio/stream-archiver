{
  "name": "composer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "yaml2json env.dev.yaml >.env.json && ./node_modules/.bin/env-cmd -f ./.env.json functions-framework --target=composeStreamArchive --port=8081",
    "test": "sh ./test.sh",
    "lint": "npx eslint . --fix && npx prettier . --write",
    "deploy:dev": "gcloud functions deploy composeStreamArchive --project chirpradiodev --gen2 --runtime=nodejs20 --region=us-central1 --env-vars-file env.dev.yaml --trigger-topic compose-stream-archive",
    "schedule:dev": "sh ./schedule.sh projects/chirpradiodev/topics/compose-stream-archive"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/storage": "^7.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.0.0",
    "prettier": "^3.2.5",
    "yaml2json": "^1.0.2"
  }
}
